{
    "manifest_version": 3,
    "name": "Form Autofill",
    "version": "1.0",
    "permissions": ["scripting", "storage"],
    "host_permissions": ["https://rezervacie.porodnicabory.sk/*"],
    "background": {
        "service_worker": "background.js",
        "persistent": true
    },
    "content_scripts": [{
        "matches": ["https://rezervacie.porodnicabory.sk/register"],
        "js": ["content.js"]
    }],
    "action": {
        "default_popup": "popup.html"
    }
}